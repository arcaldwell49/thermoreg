temp_convert = function(t = NULL,
to = "celsius",
from = "fahrenheit"){
if(to == "celsius"){
if(from == "fahrenheit"){
t2 = 5/9*(t-32)
} else if(from == "kelvin"){
t2 = t2 - 273
} else{
t2 = t
}
}
if(to == "fahrenheit"){
if(from == "celsius"){
t2 = 9/5*(t) + 32
} else if(from == "kelvin"){
t2 = 9/5*(t-273)+32
} else{
t2 = t
}
}
if(to == "kelvin"){
if(from == "fahrenheit"){
t2 = 5/9*(t-32)+273
} else if(from == "celsius"){
t2 = t2 + 273
} else{
t2 = t
}
}
